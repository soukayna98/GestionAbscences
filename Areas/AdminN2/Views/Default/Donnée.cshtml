
@{
    ViewBag.Title = "Donnée";
}





<title> Profil Employe </title>

<div class="card shadow-lg border-0 rounded-lg mt-5" style="margin-right:280px; margin-left:280px; ">
    <div class="card-header"><h3 class="text-center font-weight-blod my-4">@GestionAbscences.langagues.Resource1.donne</h3></div>
    <div class="card-body border-danger">
        <table class="table table-bordered border-success zmdi-hc-border-circle">

            <tr>
                <td class="table-warning text-center " style="font-weight:bold;">@GestionAbscences.langagues.Resource1.Nomcomplet </td>
                <td class="table-light text-center">@Session["userName"]</td>
            </tr>

            <tr>
                <td class="table-warning text-center" style="font-weight:bold;">@GestionAbscences.langagues.Resource1.Matricule</td>
                <td class="table-light text-center">@Session["matricule"]</td>
            </tr>
            <tr>
                <td class="table-warning text-center" style="font-weight:bold;">@GestionAbscences.langagues.Resource1.Affectation</td>
                <td class="table-light text-center">@Session["affectation"]</td>
            </tr>
            <tr>
                <td class="table-warning text-center" style="font-weight:bold;">@GestionAbscences.langagues.Resource1.Classe</td>
                <td class="table-light text-center">@Session["Classe"]</td>
            </tr>

            <tr>
                <td class="table-warning text-center" style="font-weight:bold;">@GestionAbscences.langagues.Resource1.Debut</td>
                <td class="table-light text-center">@Session["DateDebut"]</td>
            </tr>
            <tr>
                <td class="table-warning text-center" style="font-weight:bold;">@GestionAbscences.langagues.Resource1.Fin</td>
                <td class="table-light text-center">@Session["DateFin"]</td>
            </tr>
            <tr>
                <td class="table-warning text-center" style="font-weight:bold;">@GestionAbscences.langagues.Resource1.Nombredejourtotal</td>
                <td class="table-light text-center">@Session["nbjours"]</td>
            </tr>
            <tr>
                <td class="table-warning text-center" style="font-weight:bold;">@GestionAbscences.langagues.Resource1.Password</td>
                <td class="table-light text-center ">@Session["password"] <a class="nav-link" href="@Url.Action("changePassword","Default")">@GestionAbscences.langagues.Resource1.lien</a></td>
            </tr>


        </table>

    </div>
</div>
